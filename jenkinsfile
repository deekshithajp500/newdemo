pipeline {
    agent any
    environment {
         dockerImage=''
         registry = 'deekshithajp/image'
    }
    
      stages {
        stage('Checkout') {
          steps {
             sh "python3 tictactoe.py"
             checkout([$class: 'GitSCM', branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/deekshithajp500/newdemo.git']]])
          }
        }
          
        stage(' Build Docker image') {
          steps {
              script {
               dockerImage = docker.build registry
              }
              }
              }
      
            }
            }
