pipeline {
    agent any
    environment {
         dockerImage=''
    }
    
      stages {
        stage(' Build Docker image') {
          steps {
              script {
               dockerImage = docker build registry
              }
              }
        stage('Example') {
            steps {
                sh "python3 tictactoe.py"
                echo 'Gaming challenge'
            }
            }
            }
            
